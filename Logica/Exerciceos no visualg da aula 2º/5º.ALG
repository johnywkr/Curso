Algoritmo "semnome"

var
  A, B : Real;
inicio

  escreva("Informe um número.")
  leia(A)

  escreva("Informe outro número.")
  leia(B)

  escreva(" A Soma dos números é: ", (A + B))

  escreva(" A Subtração dos números é : ", (A - B))

  escreva(" A Multiplicação dos números é : ", (A * B))

  escreva(" A Divisão dos números é : ", (A / B))

fimalgoritmo